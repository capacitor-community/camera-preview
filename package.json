{
  "name": "@capgo/camera-preview",
  "version": "3.3.0",
  "description": "Camera preview",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api CameraPreviewPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist && cd android && ./gradlew clean && cd ..",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "author": "Martin Donadieu <martindonadieu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^4.4.0",
    "@capacitor/cli": "^4.4.0",
    "@capacitor/core": "^4.4.0",
    "@capacitor/docgen": "^0.2.0",
    "@capacitor/ios": "^4.4.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-java": "^1.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.5",
    "swiftlint": "^1.0.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapgoCameraPreview.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cap-go/camera-preview.git"
  },
  "bugs": {
    "url": "https://github.com/Cap-go/camera-preview/issues"
  }
}
